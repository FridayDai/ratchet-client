{
    "camelcase": true,
    "curly": true,
    "eqeqeq": true,
    "es3": true,
    "funcscope": true,
    "immed": true,
    "iterator": true,
    "latedef": true,
    "globals": {
        "jQuery": false,
        "RC": true,
        "_": false,
        "moment": false,
        "d3": false
    },
    "maxparams": 4,
    "maxdepth": 4,
    "maxstatements": 15,
    "maxcomplexity": 6,
    "maxlen": 120,
    "newcap": true,
    "noarg": true,
    "nonbsp": true,
    "nonew": true,
    "notypeof": true,
    "undef": true,
    "unused": true,

    "browser": true,
    "node": true
}
